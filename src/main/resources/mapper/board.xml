<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springboot_mybatis_crud.mapper.BoardMapper">

    <select id="selectBoardListCnt" parameterType="BoardDto" resultType="int">
        SELECT COUNT(*) AS TOTCNT FROM BOARD
    </select>

    <select id="selectBoardList" parameterType="BoardDto" resultType="BoardDto">
        SELECT ID, NAME, TEL, AGE, TITLE, CONTENT FROM BOARD
    </select>

</mapper>